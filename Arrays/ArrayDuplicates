//GFG - Array Duplicates

class Solution {
    public ArrayList<Integer> findDuplicates(int[] arr) {
        // code here
        ArrayList<Integer> duplicate=new ArrayList<>();
        HashSet<Integer> seen=new HashSet<>();
        
        for(int num:arr){
            if(seen.contains(num)){
                duplicate.add(num);
            }
            else{
                seen.add(num);
            }
        }
        return duplicate;
        
    }
}
